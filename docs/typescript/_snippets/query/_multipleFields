const productsCursor = catalog.findMany({
  filter: {
    op: LogicalOperator.AND,
    selectorFilters: [
      {
        brand: "adidas",
      },
      {
        op: SelectorFilterOperator.LT,
        fields: {
          price: 50,
        },
      },
    ],
  },
});

for await (const product of productsCursor) {
  console.log(product);
}
