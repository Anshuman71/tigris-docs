const productsCursor: Cursor<Catalog> = catalog.findMany({
    op: LogicalOperator.AND,
    selectorFilters: [
        {
            labels: "shoes"
        },
        {
            op: SelectorFilterOperator.GT,
            fields: {
                reviews: {
                  rating: 7
                }
            }
        }
    ]
});

for await (const product of productsCursor) {
    console.log(product);
}