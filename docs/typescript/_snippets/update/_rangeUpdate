const updateResponse: UpdateResponse = await catalog.update(
  {
    op: LogicalOperator.AND,
        selectorFilters: [
            {
                op: SelectorFilterOperator.LT,
                fields: {
                    id: 4
                }
            },
            {
                op: SelectorFilterOperator.GTE,
                fields: {
                    id: 2
                }
            }
        ]
  },
  {
    popularity: 9,
  }
);
