// insert single document
const user: User = {
  name: "Jania McGrory",
  balance: 6045.7,
};

const insertedUser: User = await users.insert(user);

// insert multiple documents
const users: Array<User> = [
  {
    name: "Jania McGrory",
    balance: 6045.7,
  },
  {
    name: "Bunny Instone",
    balance: 2948.87,
  };
];

const insertedUsers: Array<User> = await users.insertMany(users);