const eventsCursor: Cursor<ProductEvents> = topic.findMany({
    op: LogicalOperator.AND,
    selectorFilters: [
        {
            brand: "adidas"
        },
        {
            status: "sold"
        },
    ]
}, {
    include: ["name", "brand", "status"],
});

for await (const event of eventsCursor) {
    console.log(event);
}
